{
  "ver" : "1.0.0",
  "name" : "List genomes in reference databases",
  "authors" : [ "chenry", "qzhang" ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : null,
    "output" : "kbaseReportView"
  },
  "parameters" : [ {
    "id" : "refseq",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["1"],
    "field_type" : "checkbox",
    "checkbox_options":{
      "checked_value": 1,
      "unchecked_value": 0
    }
  }, {
    "id" : "ensembl",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["0"],
    "field_type" : "checkbox",
    "checkbox_options":{
      "checked_value": 1,
      "unchecked_value": 0
    }
  }, {
    "id": "phytozome",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["0"],
    "field_type" : "checkbox",
    "checkbox_options":{
      "checked_value": 1,
      "unchecked_value": 0
    }
  }, {
    "id": "updated_only",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["0"],
    "field_type" : "checkbox",
    "checkbox_options":{
      "checked_value": 1,
      "unchecked_value": 0
    }
  }, {
    "id" : "domain",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["bacteria"],
    "field_type" : "dropdown",
     "dropdown_options":{
      "options": [
        {
          "value": "bacteria",
          "display": "B (Bacteria)",
          "id": "B",
          "ui_name": "B (Bacteria)"
        },
        {
          "value": "archaea",
          "display": "A (Archaea)",
          "id": "fast",
          "ui_name": "A (Archaea)"
        },
        {
          "value": "fungi",
          "display": "F (Fungi)",
          "id": "fast",
          "ui_name": "F (Fungi)"
        },
        {
          "value": "plant",
          "display": "P (Plant)",
          "id": "fast",
          "ui_name": "P (Plant)"
        }
      ]
    }
  }],
  "job_id_output_field": "docker",
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "ReferenceDataManager",
      "method" : "list_reference_genomes",
      "input_mapping" : [
        {
          "input_parameter": "refseq",
          "target_property": "refseq"
        },
        {
          "input_parameter": "ensembl",
          "target_property": "ensembl"
        },
        {
          "input_parameter": "phytozome",
          "target_property": "phytozome"
        },
        {
          "input_parameter": "domain",
          "target_property": "domain"
        },
        {
          "input_parameter": "updated_only",
          "target_property": "updated_only"
        },
        {
          "constant_value" : "1",
          "target_property": "create_report"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        }
      ],
      "output_mapping" : [
        {
          "target_property" : "report_ref",
    	  "service_method_output_path" : [
				0,
				0
        	]
        }
      ]
    }
  }
}
