{
  "ver" : "1.0.0",
  "name" : "RAST a list of genomes in a KBase workspace and indexes in SOLR on demand",
  "authors" : [ "chenry", "qzhang" ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : null,
    "output" : "kbaseReportView"
  },
  "parameters" : [ {
    "default_values" : [""],
    "advanced" : false,
    "id" : "data",
    "allow_multiple" : false,
    "optional" : true,
    "field_type" : "text",
    "text_options" : {
      "valid_as" : "text"
    }
  }, {
    "default_values" : [],
    "advanced" : false,
    "id" : "genomes",
    "allow_multiple" : false,
    "optional" : true,
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : ["KBaseReferenceGenomeData"]
    }
  }],
  "job_id_output_field": "docker",
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "ReferenceDataManager",
      "method" : "rast_genomes",
      "input_mapping" : [
        {
          "input_parameter": "data",
          "target_property": "data"
        },
        {
          "input_parameter": "genomes",
          "target_property": "genomes"
        },
        {
          "constant_value" : "1",
          "target_property": "create_report"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        }
      ],
      "output_mapping" : [
        {
          "service_method_output_path": [0, "report_name"],
          "target_property": "report_name"
        },
        {
          "service_method_output_path": [0, "report_ref"],
          "target_property": "report_ref"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        }
      ]
    }
  }
}
