{
  "ver" : "1.0.0",
  "name" : "List reference genomes loaded in KBase",
  "authors" : [ "chenry", "qzhang" ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [ {
    "id" : "data_source",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["refseq"],
    "field_type" : "dropdown",
    "dropdown_options":{
      "options": [
        {
          "value": "refseq",
          "display": "refseq",
          "id": "refseq",
          "ui_name": "refseq"
        },
        {
          "value": "ensembl",
          "display": "ensembl",
          "id": "ensembl",
          "ui_name": "ensembl"
        },
        {
          "value": "phytozome",
          "display": "phytozome",
          "id": "phytozome",
          "ui_name": "phytozome"
        },
        {
          "value": "others",
          "display": "others",
          "id": "others",
          "ui_name": "others"
        }
      ]
    }
  }, {
    "id" : "genome_ws",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["ReferenceDataManager"],
    "field_type" : "text",
    "text_options":{
	"validate_as" : "text"
    }
  }, {
    "id": "genome_ver",
    "optional":false,
    "advanced":false,
    "allow_multiple":false,
    "default_values":["1"],
    "field_type" : "text",
    "text_options":{
	"validate_as" : "int"
    }
  }, {
    "id" : "save_date",
    "optional":true,
    "advanced":true,
    "allow_multiple":false,
    "default_values":"",
    "field_type" : "text",
    "text_options":{
	"validate_as" : "text"
    }
  }],
  "job_id_output_field": "docker",
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "ReferenceDataManager",
      "method" : "list_loaded_genomes",
      "input_mapping" : [
        {
          "input_parameter": "data_source",
          "target_property": "data_source"
        },
        {
          "input_parameter": "genome_ws",
          "target_property": "genome_ws"
        },
	{
          "input_parameter": "genome_ver",
          "target_property": "genome_ver"
        },
        {
          "input_parameter" : "save_date",
          "target_property": "save_date"
        },
        {
          "constant_value" : "1",
          "target_property": "create_report"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        }
      ],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        }
      ]
    }
  }
}
